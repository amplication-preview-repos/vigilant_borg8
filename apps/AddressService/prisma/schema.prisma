datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Address {
  address_1 String?
  address_2 String?
  city      String?
  createdAt DateTime @default(now())
  customers Json?
  id        String   @id @default(cuid())
  state     String?
  updatedAt DateTime @updatedAt
  zip       Int?
}
